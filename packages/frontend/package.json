{
  "name": "yeoman-ui-frontend",
  "displayName": "Application Wizard Frontend",
  "version": "1.14.4",
  "private": true,
  "description": "Frontend for the application wizard framework",
  "license": "Apache 2.0",
  "publisher": "SAP",
  "scripts": {
    "build": "vite build",
    "build-dev": "vite build --mode development",
    "ci": "npm-run-all clean build coverage coverage:copy",
    "clean": "shx rm -rf ./dist ./coverage",
    "coverage": "jest -w 1",
    "coverage:copy": "shx mkdir -p ../../coverage && shx cp -u ./coverage/lcov.info ../../coverage/lcov_frontend.info",
    "serve": "vite",
    "test": "jest -w 1"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ],
  "nyc": {
    "reporter": [
      "lcov",
      "text"
    ],
    "tempDirectory": "./coverage"
  },
  "dependencies": {
    "@mdi/font": "5.9.55",
    "@sap-devx/inquirer-gui": "3.0.2",
    "@sap-devx/inquirer-gui-auto-complete-plugin": "3.0.1",
    "@sap-devx/inquirer-gui-file-browser-plugin": "3.0.1",
    "@sap-devx/inquirer-gui-folder-browser-plugin": "3.0.1",
    "@sap-devx/inquirer-gui-label-plugin": "3.0.2",
    "@sap-devx/inquirer-gui-login-plugin": "3.0.1",
    "@sap-devx/inquirer-gui-tiles-plugin": "3.0.1",
    "@sap-devx/webview-rpc": "0.4.1",
    "@sap-devx/yeoman-ui-types": "^1.14.4",
    "core-js": "^3.8.3",
    "material-design-icons-iconfont": "6.1.0",
    "vue": "^3.3.4",
    "vue-loading-overlay": "^6.0.0",
    "vuetify": "^3.4.4"
  },
  "devDependencies": {
    "@babel/core": "^7.12.16",
    "@babel/preset-env": "7.11.5",
    "@vitejs/plugin-vue": "4.5.2",
    "@vue/compiler-sfc": "3.3.4",
    "@vue/test-utils": "2.4.1",
    "@vue/vue3-jest": "29.2.6",
    "babel-jest": "29.7.0",
    "eslint-plugin-vue": "^9.21.1",
    "husky": "4.2.5",
    "jest": "29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-serializer-vue": "^2.0.2",
    "jest-transform-stub": "^2.0.0",
    "mock-socket": "^9.0.2",
    "sass": "1.56.1",
    "ts-jest": "^29.1.1",
    "vite": "4.5.2"
  }
}
